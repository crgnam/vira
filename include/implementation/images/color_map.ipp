namespace vira::images {
    ColorRGB colorMap(float value, std::vector<ColorRGB>& colormap, std::vector<float>& key)
    {
        if (std::isinf(value)) {
            return ColorRGB{ 1,1,1 };
        }

        size_t index = 0;
        for (size_t i = 0; i < key.size(); ++i) {
            if (value < key[i]) {
                break;
            }
            index = i;
        }

        if (index == (key.size() - 1)) {
            return colormap[index];
        }

        float x1 = key[index];
        float x2 = key[index + 1];
        ColorRGB y1 = colormap[index];
        ColorRGB y2 = colormap[index + 1];
        ColorRGB outColor = y1 + ((value - x1) * (y2 - y1) / (x2 - x1));

        return outColor;
    };
}

